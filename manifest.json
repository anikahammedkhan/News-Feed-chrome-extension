{
    "manifest_version": 3,
    "name": "News Feed : Your Online News Paper",
    "version": "1.0.0",
    "description": "A Chrome extension for reading popular Bangla and English news paper feeds.",
    "permissions": ["activeTab"],
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon48.png"
    }
}
  